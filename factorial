import java.util.*;
class Main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number");
        int n=sc.nextInt();
        int factorial=1;
        sc.close();
        for(int i=1;i<=n;i++){
            factorial *=i;
        }System.out.println("Factorial is: " +factorial);

    }
}
