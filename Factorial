import java.util.*;
class Main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("ENTER THE NUMBER: ");
        int n=sc.nextInt();
        int factorial=1;
        for(int i=1;i<=n;i++){
            factorial *=i;
        }
        System.out.print("FACTORIAL IS :"+factorial);
    }
}
